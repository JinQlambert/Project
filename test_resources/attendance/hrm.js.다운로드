/* 연가신청서 입력 - 수정 - 수정 */
function modify_h030402() {

  obj  =  document.fm_h030402;
  
  var ymd        = obj.p_ymd.value;
  var modify_ymd = obj.p_modify_ymd.value;

  if (modify_ymd.length != 8) {
    alert("수정일자를 선택해주세요.");
    return false;
  }
  if (ymd == modify_ymd) {
    alert("기존 신청일자와 수정일자가 같습니다.");
    return false;
  }

  obj.action = './h03040201.php';
  obj.target = '_self';
  obj.method = 'POST';
  obj.submit();
}
/* 연가신청서 입력 - 삭제 */
