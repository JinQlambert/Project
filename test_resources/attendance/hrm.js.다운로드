    if ((!username || username.length > 0) && username.length < 2) {
      alert("성명은 두글자 이상 입력하세요.");
      return false;
    }
  
    search_name(Page);
  }
  else {
    obj.action = './h020200.php';
    obj.method = 'post';
    obj.target = '_self';
    obj.submit();
  }
}

