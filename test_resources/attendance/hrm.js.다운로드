    alert("시작일 또는 종료일을 입력하세요.");
    obj.p_begymd.focus();
    return false;
  }

  if (begymd > endymd) {
    alert("시작일보다 종료일이 빠릅니다. 다시 선택하세요.");
    obj.p_endymd.focus();
    return false;
  }

  var reason = obj.p_reason.value;

  if (!reason) {
    alert("사유를 입력하세요.");
    obj.p_reason.focus();
    return false;
  }

  var address = obj.p_address.value;
  
  if (!address) {
    alert("체재지를 입력하세요.");
    obj.p_address.focus();
    return false;
  }

  var tel = obj.p_tel.value;
  
  if (!tel) {
    alert("연락처를 입력하세요.");
    obj.p_tel.focus();
    return false;
  }

