/* 연가일 변경·추가 신청 - 수정 - 수정 */
function modify_h030502() {

  obj  =  document.fm_h030502;

  var gubun =  obj.p_gubun.value;

  if(!gubun || gubun<2 || gubun>4) {
    alert("1 수정할 수 없는 신청 건 입니다.");
    return false;
  }

  var id    =  obj.p_id.value;
  var seq   =  obj.p_seq.value;

  if(!id || !seq) {
    alert("2 수정할 수 없는 신청 건 입니다.");
    return false;
  }
  
  if(gubun==2||gubun==3) {
    var ymd        = obj.p_ymd.value;
    var modify_ymd = obj.p_modify_ymd.value;

    if (modify_ymd.length != 8) {
      alert("수정일자를 선택해주세요.");
      return false;
    }
    if (ymd == modify_ymd) {
      alert("기존 신청일자와 수정일자가 같습니다.");
      return false;
    }
  }

  var comment1 = obj.p_comment1.value;
  
  if (!comment1) {
    alert("사유를 입력하세요.");
    obj.p_comment1.focus();
    return false;
  }

  var comment2 = obj.p_comment2.value;
  
  if (!comment2) {
    alert("체재지(연락처)를 입력하세요.");
    obj.p_comment2.focus();
    return false;
  }

  obj.action = './h03050201.php';
  obj.target = '_self';
  obj.method = 'POST';
  obj.submit();
}
