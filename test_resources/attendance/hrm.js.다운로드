/* 공지사항조회 - 제목 클릭시 상세페이지 이동 */
function notice_show(form, id) {

  frm  =  document.getElementById(form);
  //obj.action = './h010101.php';
  frm.action = '/new/h01/h010101.php';
  frm.p_id.value = id;
  frm.method = 'post';
  frm.target = '_self';
  frm.submit();
}

