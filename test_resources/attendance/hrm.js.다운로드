/* 예외 전담근무자 등록 - 등록 - 성명 검색*/
function search_h021101(Page) {
  
  obj = document.fm_h021101;

  var username = obj.p_search_username.value;

  if (username.length > 0 && username.length < 2) {
    alert("성명은 두글자 이상 입력하세요.");
    return false;
  }

  search_name(Page);
}

/* 예외 전담근무자 등록 - 등록 - 추가 */
function insert_h021101() {

  obj  =  document.fm_h021101;

  var userid    = obj.p_search_userid.value;
  var campus    = obj.p_campus.value;
  var groupid   = obj.p_groupid.value;
  var groupname = obj.p_groupname.value;
  var usertype  = obj.p_usertype.value;
  var begymd    = obj.p_begymd.value;
  var endymd    = obj.p_endymd.value;


  if (userid.length <= 0 || campus.length <= 0 || groupid.length <= 0 || groupname.length <= 0 || usertype.length <= 0) {
    alert("근무자를 선택해주시기 바랍니다.");
    obj.p_search_username.focus();
    return false;
  }
  
  if (begymd <= 0 || endymd <= 0 ) {
    alert("예외시작일 또는 예외종료일을 선택하세요.");
    obj.p_begymd.focus();
    return false;
  }
  
  if (begymd > endymd) {
    alert("조회시작일보다 종료일이 빠릅니다. 다시 선택하세요.");
    obj.p_endymd.focus();
    return false;
  }

  obj.action = './h02110101.php';
  obj.method = 'post';
  obj.target = '_self';
  obj.submit();
}

