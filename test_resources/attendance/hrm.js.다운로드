/* 개인별 복무 현황 - 조회 */
function search_h030100(Page, Admin) {
  
  obj = document.fm_h030100;

  /*
  var username = obj.p_search_username.value;

  if (username.length < 2) {
    alert("성명은 두글자 이상 입력하세요.");
    return false;
  }

  search_name(Page);
  */

  if (Admin==1) {
    var username = obj.p_search_username.value;

    if ((!username || username.length > 0) && username.length < 2) {
      alert("성명은 두글자 이상 입력하세요.");
      return false;
    }
  
    search_name(Page);
  }
  else {
    obj.action = './h030100.php';
    obj.method = 'post';
    obj.target = '_self';
    obj.submit();
  }
}
