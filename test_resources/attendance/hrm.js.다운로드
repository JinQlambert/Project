/* 예외 교대근무자 등록 - 조회 */
function search_h021200() {
  
  obj = document.fm_h021200;
   
  var begym = (obj.p_search_begyear.value.concat(lpad(obj.p_search_begmonth.value, 2, 0)))*1;
  var endym = (obj.p_search_endyear.value.concat(lpad(obj.p_search_endmonth.value, 2, 0)))*1;

  if (begym > endym) {
    alert("조회시작일보다 종료일이 빠릅니다. 다시 선택하세요.");
    obj.p_search_endmonth.focus();
    return false;
  }

  var username = obj.p_search_username.value;

  if (username.length > 0) {
    if (username.length < 2) {
      alert("성명은 두글자 이상 입력하세요.");
      return false;
    }
    // 다른 조회조건 초기화
    obj.p_search_campus.value = 0;
    obj.p_search_groupid.value = 'ALL';
    obj.p_search_usertype.value = 0;
  }

  obj.action = './h021200.php';
  obj.method = 'post';
  obj.target = '_self';
  obj.submit();
}

/* 예외 교대근무자 등록 - 등록 - 성명 검색*/
