function logout() {
    location.replace("../logout.php");
}
function goBack() {
    window.history.back();
}

function numkeyCheck(event) { 
  event = event || window.event;
  var keyID = (event.which) ? event.which : event.keyCode;
  if( ( keyID >=48 && keyID <= 57 ) || ( keyID >=96 && keyID <= 105 ) || keyID == 8 || keyID == 46 || keyID == 37 || keyID == 39 )
  {
    return;
  }
  else
  {
    return false;
  }
}

function datePicker(id, Mindate, Maxdate) {

  var temp = $('#'+id);

  $(temp).datepicker({
      dateFormat       :'yymmdd',
      showOtherMonths  : true,
      selectOtherMonths: true,      
      dayNamesMin      : ['일', '월', '화', '수', '목', '금', '토' ],
      monthNamesShort  : ['1월','2월','3월','4월','5월','6월','7월','8월','9월','10월','11월','12월'],
      showButtonPanel  : true,
      minDate          : Mindate,
      maxDate          : Maxdate
      //closeText: 'Clear',
      //onClose: function(dateText, inst) {
      //  if ($(window.event.srcElement).hasClass('ui-datepicker-close')) {
      //    document.getElementById(id).value = '';
      //  }
      //}
  });
  
  $.datepicker._gotoToday = function(temp) { 
    $(temp).datepicker('setDate', new Date()).datepicker('hide').blur(); 
  };
    
  $('img.ui-datepicker-trigger').css({'position':'relative','left':'2px','bottom':'2px'});
}


