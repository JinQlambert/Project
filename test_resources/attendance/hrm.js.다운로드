/* 부서별 관리자 등록 - 등록 - 추가 */
function insert_h060201() {

  obj  =  document.fm_h060201;

  var userid    = obj.p_search_userid.value;

  if (userid.length <= 0) {
    alert("근무자를 선택해주시기 바랍니다.");
    obj.p_search_username.focus();
    return false;
  }

  var ip    = obj.p_ip.value;
  
  if (ip.length <= 0) {
    alert("IP 입력은 필수입니다.");
    obj.p_ip.focus();
    return false;
  }


  obj.action = './h06020101.php';
  obj.method = 'post';
  obj.target = '_self';
  obj.submit();
}

