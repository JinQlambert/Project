  obj.p_userid.value = Userid;
  obj.p_ymd.value = Ymd;

  con = confirm('삭제하시겠습니까?');
  if (con == 1) {
    obj.action = './h030403.php';
    obj.target = '_self';
    obj.method = 'POST';
    obj.submit();
  }
  else {
    return false;
  }
}
/* 연가신청서입력기간 - 추가 */
function insert_h060300() {

  obj  =  document.fm_h060300_2;

  var insert_begymd = obj.p_insert_begymd.value;
  var insert_endymd = obj.p_insert_endymd.value;

  if (insert_begymd > insert_endymd) {
    alert("입력 및 제출기한 시작일보다 종료일이 빠릅니다. 다시 선택하세요.");
    obj.p_insert_endymd.focus();
    return false;
  }

  var begymd = obj.p_begymd.value;
  var endymd = obj.p_endymd.value;

  if (begymd > endymd) {
    alert("사용기한 시작일보다 종료일이 빠릅니다. 다시 선택하세요.");
    obj.p_endymd.focus();
    return false;
  }

  obj.action = './h060301.php';
  obj.method = 'post';
  obj.target = '_self';
  obj.submit();
}
