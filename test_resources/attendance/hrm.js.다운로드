  obj.method = 'post';
  obj.submit();
}

/* 성명 검색 */ 
function search_name(Page) {
  
  obj = (new Function( 'return ' + 'document.fm_'+Page))(); 

  var username = obj.p_search_username.value; // 성명

  //console.log("search_name username : "+username);

  //if (username.length < 2) {
  //  alert("성명은 두글자 이상 입력하세요.");
  //  return false;
  //}
  var getUserid = function(callback) {
    $.ajax({
      url  : "/new/lib/search_userid.php",
      type : "GET",
      data : "targetname="+username,
      error:function(){
        alert("성명을 다시 입력해주시기 바랍니다.");
      },
      success:function(data) {
        // 0명
        if(data=="nodata"){
          alert("검색 대상에 존재하지 않습니다.");
        }
        // 2명 이상
        else if(data=="multi"){
          console.log("multi");
          samename_popup(Page, username); // 성명 선택 레이어 팝업
        }
        // 1명
        else {
          json = eval("(" + data + ")");
          $.each(json.list, function(key,state){
            temp = state;
            callback(temp);
          });
        }
      }
    });
  }
  
  // 1명
  getUserid(function(Temp) {

    //console.log(Temp);
    obj.p_search_userid.value   = Temp.userid;
    obj.p_search_username.value = Temp.username;

    //obj.action = './h020100.php';
    obj.action = './'+Page+'.php';
    obj.method = 'post';
    obj.target = '_self';
    obj.submit();
  });
}

/* 성명 선택 레이어팝업 */
function samename_popup(Page, Username) {

  var temp = $('#popup_layer1');    //레이어의 id를 temp변수에 저장
  var bg = temp.prev().hasClass('popup_bg');  //dimmed 레이어를 감지하기 위한 boolean 변수

  $.ajax({
    url:'/new/lib/search_samename.php',
    type:'get',
    dataType:'html',
    data:'targetname='+Username,
    error:function(){
      alert('Error!!');
    },
    success:function(data){
      //alert('Success!!')
      //var test = data;
      $('#pop-table').html(data);
    }
  });
  
  // 바탕
  if(bg){
    $('#popup_layer1').fadeIn();
    $('#popup_bg1').css('display', 'block');
  }
  else{
    temp.fadeIn();  //bg 클래스가 없으면 일반레이어로 실행한다.
  }

  // 화면의 중앙에 레이어를 띄운다.
  var iHeight = (document.body.clientHeight / 2) - temp.height() / 2 + document.body.scrollTop;
  var iWidth = (document.body.clientWidth / 2) - temp.width() / 2 + document.body.scrollLeft;

  if (iHeight > 0 && iWidth > 0) {
    //temp.css('top',  iHeight+'px');
    //temp.css('left', iWidth+'px');
    temp.css('top',  '50px');
    temp.css('left', '50px');
  }
  else {
    temp.css('top',  '50px');
    temp.css('left', '50px');
  }

  // 확인
  temp.find('a.btn-submit').click(function(e){

    // Get userid, username
    var gobj = document.fm_search_samename;

    if(!$(':input:radio[name=trow]:checked').val()) {
      alert("대상자를 선택해주세요.");
      return false;
    }

    for (var i=0; i<$(':input:radio[name=trow]').length;i++){
      if (gobj.trow[i].checked == true) {
        var guserid   = gobj.userid[i].value;
        var gusername = gobj.username[i].value;
      }
    }

    // Set userid, username
    obj.p_search_userid.value   = guserid;
    obj.p_search_username.value = gusername;

    if(bg){
      $('#popup_layer1').fadeOut();
      $('#popup_bg1').css('display', 'none');
    }else{
      temp.fadeOut();   //레이어가 사라진다.
    }

    //obj.action = './h020100.php';
    obj.action = './'+Page+'.php';
    obj.method = 'post';
    obj.target = '_self';
    obj.submit();
  });
    
  // 닫기
  temp.find('a.btn-close').click(function(e){
    if(bg){
      $('#popup_layer1').fadeOut();
      $('#popup_bg1').css('display', 'none');
    }else{
      temp.fadeOut();   //레이어가 사라진다.
    }
  });

  $('#popup_layer1 #popup_bg1').click(function(e){
    $('#popup_layer1').fadeOut();
  });
}

