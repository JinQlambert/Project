/* 연가일 변경·추가 신청 - 추가신청 - 추가 */
function insert_h030501() {
  obj  =  document.fm_h030501;

  var begymd = obj.p_begymd.value;
  var endymd = obj.p_endymd.value;

  if (begymd > endymd) {
    alert("시작일보다 종료일이 빠릅니다. 다시 선택하세요.");
    obj.p_endymd.focus();
    return false;
  }

  var comment1 = obj.p_comment1.value;
  
  if (!comment1) {
    alert("사유를 입력하세요.");
    obj.p_comment1.focus();
    return false;
  }

  var comment2 = obj.p_comment2.value;
  
  if (!comment2) {
    alert("체재지(연락처)를 입력하세요.");
    obj.p_comment2.focus();
    return false;
  }

  obj.action = './h03050101.php';
  obj.method = 'post';
  obj.target = '_self';
  obj.submit();
}
