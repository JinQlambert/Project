/* 출장 신청 - 추가 - 신청 */
function insert_h031301() {
  obj  =  document.fm_h031301;

  var begymd = obj.p_begymd.value;
  var endymd = obj.p_endymd.value;
  
  if (!begymd || !endymd) {
    alert("시작일 또는 종료일을 입력하세요.");
    obj.p_begymd.focus();
    return false;
  }

  if (begymd > endymd) {
    alert("시작일보다 종료일이 빠릅니다.");
    obj.p_endymd.focus();
    return false;
  }

  var nights = obj.p_nights.value;
  var days   = obj.p_days.value;
  var times  = obj.p_times.value;
  var counts = obj.p_counts.value;
  
  if(nights.match(/[^0-9]/) || days.match(/[^0-9]/) || times.match(/[^0-9]/) || counts.match(/[^0-9]/)) {
    alert("일수, 참여시간, 참여횟수는 숫자만 입력 가능합니다.");
    return false;
  }

  var subject = obj.p_subject.value;

  if (!subject) {
    alert("출장명(용무)를 입력하세요.");
    obj.p_subject.focus();
    return false;
  }

  var ju_group = obj.p_ju_group.value;

  if (!ju_group) {
    alert("추관기관를 입력하세요.");
    obj.p_ju_group.focus();
    return false;
  }

  var address = obj.p_address.value;

  if (!address) {
    alert("출장지를 입력하세요.");
    obj.p_address.focus();
    return false;
  }

  obj.action = './h03130101.php';
  obj.method = 'post';
  obj.target = '_self';
  obj.submit();
}
/* 출장명령서 - 상세조회및수정 */ 
function m_modify_h031300(Schoolyear, id) {

  obj  =  document.fm_h031300;

  obj.p_schoolyear.value = Schoolyear;
  obj.p_id.value         = id;

  obj.action = './h031302.php';
  obj.target = '_self';
  obj.method = 'POST';
  obj.submit();
}
/* 출장명령서 - 상세조회및수정 - 저장  */
function modify_h031302() {

  obj  =  document.fm_h031302;
  
  // 출장자 리스트
  var arrVal = new Array();

  for (var i=0;i<$("#selectBox option").size();i++) {
    arrVal[i] = $("#selectBox option:eq("+i+")").val();
  }

  obj.p_tempVal.value = arrVal;

  var begymd = obj.p_begymd.value;
  var endymd = obj.p_endymd.value;
  
  if (!begymd || !endymd) {
    alert("시작일 또는 종료일을 입력하세요.");
    obj.p_begymd.focus();
    return false;
  }

  if (begymd > endymd) {
    alert("시작일보다 종료일이 빠릅니다.");
    obj.p_endymd.focus();
    return false;
  }

  var nights = obj.p_nights.value;
  var days   = obj.p_days.value;
  var times  = obj.p_times.value;
  var counts = obj.p_counts.value;
  
  if(nights.match(/[^0-9]/) || days.match(/[^0-9]/) || times.match(/[^0-9]/) || counts.match(/[^0-9]/)) {
    alert("일수, 참여시간, 참여횟수는 숫자만 입력 가능합니다.");
    return false;
  }

  obj.action = './h03130201.php';
  obj.method = 'post';
  obj.target = '_self';
  obj.submit();
}
/* 출장명령서 - 상세조회및수정 - 파일삭제 */
function deleteFile_h031302() {

  obj  =  document.fm_h031302;

  con = confirm('기존 첨부파일 삭제하시겠습니까?');
  if (con == 1) {
    obj.action = './h03130202.php';
    obj.target = '_self';
    obj.method = 'POST';
    obj.submit();
  }
  else {
    return false;
  }
}
