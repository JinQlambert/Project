/* 공휴일관리 - 추가 */
function insert_h060100() {

  obj  =  document.fm_h060100_2;

  var ymd      = obj.p_ymd.value;
  var comments = obj.p_comment.value;

  if (ymd.length <= 0) {
    alert("연월일을 선택해주시기 바랍니다.");
    obj.p_ymd.focus();
    return false;
  }

  if (comments.length <= 0) {
    alert("비고를 선택해주시기 바랍니다.");
    obj.p_comment.focus();
    return false;
  }

  obj.action = './h060101.php';
  obj.method = 'post';
  obj.target = '_self';
  obj.submit();
}
/* 공휴일관리 - 수정 */
function modify_h060100(No) {

  obj  =  document.fm_h060100;

  obj.p_no.value    = No;

  con = confirm('수정하시겠습니까?');
  if (con == 1) {
    obj.action = './h060102.php';
    obj.target = '_self';
    obj.method = 'POST';
    obj.submit();
  }
  else {
    return false;
  }
}
/* 공휴일관리 - 삭제 */
function delete_h060100(No) {

  obj  =  document.fm_h060100;

  obj.p_no.value    = No;

  con = confirm('삭제하시겠습니까?');
  if (con == 1) {
    obj.action = './h060103.php';
    obj.target = '_self';
    obj.method = 'POST';
    obj.submit();
  }
  else {
    return false;
  }
}

/* 부서별 관리자 등록 - 조회 */
function search_h060200() {

  obj  =  document.fm_h060200;

  obj.action = './h060200.php';
