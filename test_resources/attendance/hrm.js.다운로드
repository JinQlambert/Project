/* 개인별 근태 현황 - 조회 
   Admin 1=관리자,그외=일반사용자 */
function search_h020100(Page, Admin) {
  obj = document.fm_h020100;
  
  var begymd = obj.p_search_begymd.value;
  var endymd = obj.p_search_endymd.value;


  if (begymd > endymd) {
    alert("조회시작일보다 종료일이 빠릅니다. 다시 선택하세요.");
    obj.p_search_endymd.focus();
    return false;
  }
  
  if (Admin==1) {
    var username = obj.p_search_username.value;

    if ((!username || username.length > 0) && username.length < 2) {
      alert("성명은 두글자 이상 입력하세요.");
      return false;
    }
  
    search_name(Page);
  }
  else {
    obj.action = './h020100.php';
    obj.method = 'post';
    obj.target = '_self';
    obj.submit();
  }
}

/* 개인별 근태 현황 - 저장 */
function save_h020100(no){

  obj = document.fm_h020100;
  obj.p_no.value = no;
  
  obj.action = './h020101.php';
  obj.target = '_self';
  obj.method = 'POST';
  obj.submit();
}

/* 개인별 출근부 출력 - 조회 
