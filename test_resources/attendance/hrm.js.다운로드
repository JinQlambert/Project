/* 예외 전담근무자 등록 - 조회 */
function search_h021100() {
  
  obj = document.fm_h021100;
   
  var begymd = obj.p_search_begymd.value;
  var endymd = obj.p_search_endymd.value;

  if (begymd > endymd) {
    alert("조회시작일보다 종료일이 빠릅니다. 다시 선택하세요.");
    obj.p_search_endymd.focus();
    return false;
  }

  var username = obj.p_search_username.value;

  if (username.length > 0) {
    if (username.length < 2) {
      alert("성명은 두글자 이상 입력하세요.");
      return false;
    }
    // 다른 조회조건 초기화
    obj.p_search_campus.value = 0;
    obj.p_search_groupid.value = 'ALL';
    obj.p_search_usertype.value = 0;
    obj.p_search_code.value = 0;
  }

  obj.action = './h021100.php';
  obj.method = 'post';
  obj.target = '_self';
  obj.submit();
}

