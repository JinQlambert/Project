function insert_h031101() {
  obj  =  document.fm_h031101;

  var begymd = obj.p_begymd.value;
  var endymd = obj.p_endymd.value;
  
  if (!begymd || !endymd) {
    alert("시작일 또는 종료일을 입력하세요.");
    obj.p_begymd.focus();
    return false;
  }

  if (begymd > endymd) {
    alert("시작일보다 종료일이 빠릅니다. 다시 선택하세요.");
    obj.p_endymd.focus();
    return false;
  }

  var reason = obj.p_reason.value;

  if (!reason) {
    alert("병명을 입력하세요.");
    obj.p_reason.focus();
    return false;
  }

  var address = obj.p_address.value;
  
  if (!address) {
    alert("치료장소(병원명)을 입력하세요.");
    obj.p_address.focus();
    return false;
  }

  var tel = obj.p_tel.value;
  
  if (!tel) {
    alert("연락처를 입력하세요.");
    obj.p_tel.focus();
    return false;
  }

  obj.action = './h03110101.php';
  obj.method = 'post';
  obj.target = '_self';
  obj.submit();
}
